{
  "name": "wildworld",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/xizhensun/wildworld.git",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^13.13.0",
    "nodemon": "^2.0.3",
    "ts-node": "^8.8.2"
  },
  "dependencies": {
    "typescript": "^3.8.3",
    "express": "^4.17.1",
    "@colyseus/monitor": "^0.12.1",
    "colyseus": "^0.12.4",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "serve-index": "^1.8.0",
    "superagent": "^3.8.1"
  },
  "scripts": {
    "prepare": "npm run gcp-build",
    "pretest": "npm run gcp-build",
